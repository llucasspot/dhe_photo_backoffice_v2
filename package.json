{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "imports": {
    "#assets": "src/assets",
    "#components": "src/components",
    "#features/auth": "src/features/auth",
    "#features/auth/i18n": "src/features/auth/i18n",
    "#features/auth/domain": "src/features/auth/domain",
    "#features/auth/react": "src/features/auth/react",
    "#features/dashboard": "src/features/dashboard",
    "#features/home": "src/features/home",
    "#features/projects": "src/features/projects",
    "#features/schools": "src/features/schools",
    "#features/settings": "src/features/settings",
    "#features/settings/i18n": "src/features/settings/i18n",
    "#layout": "src/layout",
    "#di/domain": "./src/lib/di/src/domain",
    "#di/infra": "./src/lib/di/src/infra",
    "#di/react": "./src/lib/di/src/react",
    "#di": "./src/lib/di/src",
    "#i18n/domain": "./src/features/_/i18n/src/domain",
    "#i18n/infra": "./src/features/_/i18n/src/infra",
    "#i18n/react": "./src/features/_/i18n/src/react",
    "#i18n": "./src/features/_/i18n/src",
    "#routing/domain": "./src/features/_/routing/domain",
    "#routing/infra": "./src/features/_/routing/infra",
    "#routing/react": "./src/features/_/routing/react",
    "#routing": "./src/features/_/routing",
    "#core/domain": "./src/features/_/core/domain",
    "#core/react": "./src/features/_/core/react"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "check:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "check:format": "prettier --write \"src/**/*.ts\"",
    "check:typecheck": "tsc --noEmit -p ./tsconfig.app.json",
    "check:all": "npm run check:lint && npm run check:format && npm run check:typecheck"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-router": "^1.15.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^14.1.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@tanstack/router-devtools": "^1.81.14",
    "@tanstack/router-plugin": "^1.81.9",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.12.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10"
  }
}
